{
  "name": "keepalive-tabs-kit",
  "version": "0.2.0",
  "license": "MIT",
  "author": "aiyoudiao",
  "repository": {
    "type": "git",
    "url": "https://github.com/aiyoudiao/keepalive-tabs-kit.git"
  },
  "bugs": {
    "url": "https://github.com/aiyoudiao/keepalive-tabs-kit/issues"
  },
  "homepage": "https://github.com/aiyoudiao/keepalive-tabs-kit#readme",
  "type": "module",
  "main": "./dist/lib.js",
  "module": "./dist/lib.js",
  "types": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.js",
      "default": "./dist/lib.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:lib && tsc -p tsconfig.build.json && vite build",
    "build:lib": "tsc -p tsconfig.lib.json && cp src/style.css dist/style.css",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "test": "vitest run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=5",
    "@dnd-kit/core": ">=6",
    "@dnd-kit/sortable": ">=10",
    "@dnd-kit/utilities": ">=3",
    "antd": ">=5",
    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": ">=6"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.6.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^5.1.2",
    "antd": "^5.24.1",
    "jsdom": "^26.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "storybook": "^8.6.14",
    "typescript": "~5.6.2",
    "vite": "^5.4.21",
    "vitest": "^3.2.4"
  }
}
